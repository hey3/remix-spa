{
  "name": "remix-spa",
  "version": "1.0.0",
  "description": "Remix SPA Sample",
  "keywords": [
    "react",
    "typescript",
    "remix"
  ],
  "homepage": "https://github.com/hey3/remix-spa#readme",
  "bugs": {
    "url": "https://github.com/hey3/remix-spa/issues"
  },
  "license": "MIT",
  "author": "hey3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey3/remix-spa.git"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@9.1.2",
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev",
    "build": "remix vite:build",
    "preview": "vite preview",
    "format": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write --ignore-unknown",
    "lint": "pnpm lint:eslint && pnpm lint:style",
    "lint:fix": "pnpm lint:eslint-fix && pnpm lint:style-fix",
    "lint:eslint": "eslint ./**/*.{js,ts,tsx}",
    "lint:eslint-fix": "eslint --fix ./**/*.{js,ts,tsx}",
    "lint:style": "stylelint './**/*.css'",
    "lint:style-fix": "stylelint --fix './**/*.css'",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit --incremental false"
  },
  "dependencies": {
    "@remix-run/node": "2.9.2",
    "@remix-run/react": "2.9.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "8.57.0",
    "@remix-run/dev": "2.9.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-unused-imports": "4.0.0",
    "globals": "15.3.0",
    "prettier": "3.2.5",
    "stylelint": "16.5.0",
    "stylelint-config-recess-order": "5.0.1",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.11.0",
    "vite": "5.2.12"
  }
}
